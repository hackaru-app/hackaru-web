{
  "name": "hackaru-web",
  "description": "Hackaru web server",
  "author": "ktmouk <ktmouk@gmail.com>",
  "private": true,
  "scripts": {
    "license-third-party": "npm-license-crawler --onlyDirectDependencies --omitVersion --json assets/licenses.json",
    "dev": "nuxt",
    "production": "nuxt build && nuxt start",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore",
    "test": "jest",
    "postinstall": "yarn license-third-party"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/sentry": "^3.2.4",
    "@sentry/browser": "^5.6.3",
    "@sentry/integrations": "^5.15.4",
    "axios": "^0.19.2",
    "body-scroll-lock": "^3.0.1",
    "camelcase-keys": "^6.1.2",
    "chart.js": "^2.9.3",
    "color": "^3.1.0",
    "date-fns": "^2.9.0",
    "file-saver": "^2.0.2",
    "hh-mm-ss": "^1.2.0",
    "jwt-decode": "^2.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.merge": "^4.6.1",
    "lodash.omit": "^4.5.0",
    "lodash.union": "^4.6.0",
    "lodash.uniqby": "^4.7.0",
    "mezr": "^0.6.2",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "normalizr": "^3.6.0",
    "nuxt": "^2.10.2",
    "nuxt-env": "^0.1.0",
    "nuxt-i18n": "^6.4.1",
    "query-string": "^6.8.3",
    "sass-loader": "^8.0.2",
    "snakecase-keys": "^3.1.2",
    "style-loader": "^0.23.1",
    "v-tooltip": "^2.0.3",
    "vue-chartjs": "^3.5.0",
    "vue-feather-icons": "^5.0.0",
    "vue-gtm": "^2.2.0",
    "vue-js-modal": "^1.3.27",
    "vue-plugin-load-script": "^1.2.0",
    "vue-scrollto": "^2.17.1",
    "vue-timers": "^2.0.4",
    "vue2-animate": "^2.1.3",
    "vuex-persistedstate": "^2.7.0"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3333"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.7.7",
    "@nuxtjs/style-resources": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.0.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^4.0.0",
    "husky": "^4.0.6",
    "jest": "^25.1.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-localstorage-mock": "^2.4.0",
    "jest-serializer-vue": "^2.0.2",
    "jsonwebtoken": "^8.5.0",
    "lint-staged": "^9.4.2",
    "match-media-mock": "^0.1.1",
    "mockdate": "^2.0.2",
    "npm-license-crawler": "mwittig/npm-license-crawler#master",
    "prettier": "^1.16.1",
    "vue-jest": "^3.0.5",
    "vuex-mock-store": "^0.0.7"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock",
      "jest-localstorage-mock",
      "./__tests__/__setups__/config.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "testPathIgnorePatterns": [
      "/__tests__/__setups__/",
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!nuxt.config.js",
      "!static/sw.js",
      "!coverage/**",
      "!plugins/*",
      "!**/node_modules/**",
      "!**/__tests__/__setups__/**"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "clearMocks": true
  }
}
